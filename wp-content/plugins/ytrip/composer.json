{
    "name": "calldigital/ytrip",
    "description": "Professional travel/tourism booking system for WordPress with Codestar Framework integration",
    "version": "1.0.0",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mohamed Ahmed",
            "email": "contact@calldigitalnow.com",
            "homepage": "https://zakharioustours.de"
        }
    ],
    "homepage": "https://zakharioustours.de",
    "support": {
        "issues": "https://github.com/calldigital/ytrip/issues"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "travel",
        "booking",
        "tourism",
        "tours",
        "woocommerce"
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.0"
    },
    "autoload": {
        "classmap": [
            "includes/",
            "admin/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
